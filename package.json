{
  "name": "math",
  "private": true,
  "version": "0.1.0",
  "description": "Gamedev math libary",
  "type": "module",
  "keywords": [
    "math",
    "gamedev"
  ],
  "author": "Marco Kammer",
  "homepage": "https://github.com/kamm3r/math",
  "repository": "https://github.com/kamm3r/math",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/math.d.mts",
        "default": "./dist/math.mjs"
      },
      "types": "./dist/math.d.ts",
      "module": "./dist/math.js",
      "default": "./dist/math.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/math.js",
  "types": "./dist/math.d.ts",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "clean": "rm -rf dist",
    "build": "pnpm clean && vite build && tsc",
    "build:tsc": "pnpm clean && tsc",
    "build:fast": "pnpm build && pnpm typecheck",
    "benchmark": "./benchmark"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  }
}
